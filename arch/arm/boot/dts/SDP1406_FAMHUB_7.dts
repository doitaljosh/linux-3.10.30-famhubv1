		samsung,i2c-max-bus-freq = <400000>;
		samsung,i2c-irq-status-reg = <0x001901f4>;
		samsung,i2c-cpu-affinity = <3>;
		samsung,scl-gpio = <&gpio16 7 1>;
		samsung,sda-gpio = <&gpio17 0 1>;
		samsung,i2c-channel-number = <3>;
	};

	/* I2C-4 */
	i2c@00190180 {
		compatible = "samsung,sdp-i2c";
		status = "okay";
		samsung,i2c-sda-delay = <1>;
		samsung,i2c-slave-addr = <0x4a>;
		samsung,i2c-max-bus-freq = <400000>;
		samsung,i2c-irq-status-reg = <0x001901f4>;
		samsung,i2c-cpu-affinity = <3>;
		samsung,scl-gpio = <&gpio17 7 1>;
		samsung,sda-gpio = <&gpio18 0 1>;
		samsung,i2c-channel-number = <7>;
		samsung,hdmi-disable = <1>;

		s24c512c@50 {
			compatible = "sii,s24c512c";
			reg = <0x50>;
			samsung,eeprom-wp = <&gpio12 3 1>;
		};
	};

	/* I2C-5 */
	i2c@001901a0 {
		status = "okay";
