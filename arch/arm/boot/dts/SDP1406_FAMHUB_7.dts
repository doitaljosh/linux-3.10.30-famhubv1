	   usben {
		   status = "okay";
		   samsung,usb-enable = <&gpio13 1 0>;
	   };

	   usb_ocp {
			status = "okay";
			compatible = "samsung,usb_ocp";
			extint-reg = <0x005C104C>;
			camera_ocp_int = <0>;  // Camera
			usb_debug_ocp_int = <17>; // USB debug
			bt_ocp_int = <19>; // Blue Tooth
			st_ocp_int = <21>; // Smart Things
	   };

