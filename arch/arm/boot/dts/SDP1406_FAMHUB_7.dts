/dts-v1/;
#include "sdp1406.dtsi"

/ {
	model = "Samsung FAMHUB_7 SDP1406 SoC";
	compatible = "samsung,dtv", "samsung,sdp1406fhd";

	memory {
		device_type = "memory";
		reg = <0x1000000 0x1F000000>, <0x22800000 0x12800000>, <0x40000000 0x17000000>;
	};

	chosen {
		bootargs="root=/dev/mmcblk0p12 rootfstype=ext4 rw rootwait INITRD_PARAM_GO_HERE console=ttyS0,115200N8 earlyprintk sdp_sparsemem vmalloc=550M panic=10";
	};

	fixed-rate-clocks {
		fin {
			compatible = "samsung,sdp-clock-fin";
			clock-frequency = <24576000>;
		};
	};

	mmc {
		tuning_sam_hs400_default = <0x001000A8 0x000000CF 0x00000041>,
								 <0x00100188 0x000003FF 0x000000B0>;/* fhd read dqs delay(44step) */

		//		hs200;
		//		hs200-tuning;
		hs400;
	};

	serial@00190A00 {
		status = "okay";
	};

	serial@00190A40 {
		status = "okay";
	};

	serial@00190A80 {
		status = "okay";
	};

	dummy@00190AC0 {
		status = "okay";
		compatible = "samsung,sdp1304-uart-micom";
	};
/*
	serial@00190AC0 {
		status = "okay";
	};

	serial@00191400 {
		status = "okay";
	};
*/
	
	   rtc {
	   compatible = "samsung,dummy-rtc";
	   };


	pinctrl{
		compatible = "samsung,sdp-pinctrl";
		reg = <0x005C1000 0x230>;
		start-offset = <0x118>;
		nr-banks = <23>;
		model-sel = <1>;
	};

	leds {
		compatible = "gpio-leds";
		alive_led@0 {
			label = "Cpu-Alive-led";
			gpios = <&gpio2 1 1>;
			linux,default-trigger = "timer";
		};
	};

	sdp_messagebox@800100 {
		compatible = "samsung,sdp-messagebox";
		reg = <0x800100 0x500>;
		interrupts = <0 83 0>;
	};

	/* I2C-0 */
	i2c@00190100 {
		compatible = "samsung,sdp-i2c";
		status = "okay";
		samsung,i2c-sda-delay = <1>;
		samsung,i2c-slave-addr = <0x68>;
		samsung,i2c-max-bus-freq = <400000>;
		samsung,i2c-irq-status-reg = <0x001901f4>;
		samsung,i2c-cpu-affinity = <3>;
		samsung,scl-gpio = <&gpio16 1 1>;
		samsung,sda-gpio = <&gpio16 2 1>;
		samsung,i2c-channel-number = <0>;
		samsung,hdmi-disable = <1>; 	//hdmi (1 : disable, 0: enable)
		initregs = <0x005C104C 0x00040000 0x00040000>;/*EXTINT[18] SEL*/ 			

		atmel-mxt_t@4a {
			compatible = "atmel,mxt_t";
			reg = <0x4a>;
			atmel,boot_addr = <0x26>;
			atmel,gpio_reset = <&gpio1 4 1>;
			atmel,gpio_irq = <&gpio6 5 1>;
			atmel,x-size = <4096>;
			atmel,y-size = <4096>;
			atmel,num_xnode = <39>;
			atmel,num_ynode = <68>;
			atmel,num_touchkey = <5>;
		};
	};


	/* I2C-1 */
	i2c@00190120 {
		compatible = "samsung,sdp-i2c";
		status = "okay";
		samsung,i2c-sda-delay = <1>;
		samsung,i2c-slave-addr = <0x40>;
		samsung,i2c-max-bus-freq = <400000>;
		samsung,i2c-irq-status-reg = <0x001901f4>;
		samsung,i2c-cpu-affinity = <3>;
		samsung,scl-gpio = <&gpio16 3 1>;
		samsung,sda-gpio = <&gpio16 4 1>;
		samsung,i2c-channel-number = <1>;
		samsung,hdmi-disable = <1>;
	};

	/* I2C-2 */
	i2c@00190140 {
		compatible = "samsung,sdp-i2c";
		status = "okay";
		samsung,i2c-sda-delay = <1>;
		samsung,i2c-slave-addr = <0x3a>, <0x11>;
		samsung,i2c-max-bus-freq = <400000>;
		samsung,i2c-irq-status-reg = <0x001901f4>;
		samsung,i2c-cpu-affinity = <3>;
		samsung,scl-gpio = <&gpio16 5 1>;
		samsung,sda-gpio = <&gpio16 6 1>;
		samsung,i2c-channel-number = <2>;

		msp430@48 {
			compatible = "ti,msp430";
			reg = <0x48>;
		};
