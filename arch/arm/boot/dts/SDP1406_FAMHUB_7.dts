	ssp {
		status = "okay";
		compatible = "samsung,sdp-spi";
		reg = <0x190200 0x20>;
		interrupts = <0 38 0>;
		clocks = <&apb_pclk>;
		clock-names = "apb_pclk";
		//num-chipselect = <0>;
		mode-gpios = <&gpio12 4 0>;
		cs-gpios = <&gpio12 5 0>;

		initregs = <0x005C1040 0x00080000 0x00080000>,/*PAD SPI_SEL*/
				   <0x005C1160 0x03000000 0x01000000>,
				   <0x005C11B4 0x00000003 0x00000001>, 
				   <0x005C104C 0x00020000 0x00020000>,/*EXTINT[17] SEL*/ 
				   <0x005C1048 0x00000100 0x00000100>;/*I2S MCLK SEL*/ 
		max-clock-limit = <27000000>;
		bus-num = <0x0>;

		yamaha-ymu831@0 {
			compatible = "yamaha,ymu831";
			spi-max-frequency = <10000000>;
			reg = <0>;
			gpio_reset = <&gpio3 6 0>;
			//ext-clock = <27000000>;
			//ext-reg = <0x005C104C>;  /*EXTINT_SEL*/
			//ext-int = <19>;
		};
	};

	panel: panel {
		compatible = "samsung,sdp1202-panel";
	};

	dummy_panel {
		compatible = "dummy_panel";

		panel = <&panel>;

		display-timings {
			native-mode = <&timing0>;

			timing0: timing-0 {
				clock-frequency = <0>;
				hactive = <1920>;
				vactive = <1080>;
				hfront-porch = <0>;
				hback-porch = <0>;
				hsync-len = <0>;
				vfront-porch = <0>;
				vback-porch = <0>;
				vsync-len = <0>;
		 	};
		};
	};

